
<div class="contact">     
	<h1>Fescha Austria</h1>
		<p>Neunkirchnerstasse 44C, 4<br> 2700 Wiener Neustadt<br>Austria</p>
</div>
<div class="row-contact">
  <%= flash[:notice] %>
  <%= form_with(url:"/static_pages/thank_you", method: "post", local: true) do |form| %>

  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control", required: true, placeholder: "Enter Name" %>
  </div>
  <div class="form-group">
    <%= form.label :email %>
    <%= form.text_field :email, class: "form-control", required: true, placeholder: "Enter Email" %>
  </div>
  <div class="form-group">
    <%= form.label :message %>
    <%= form.text_area :message, class: "form-control", required: true, placeholder: "Enter Message", rows: 5 %>
  </div>
</div>
<%= form.submit "Send", class: "form_label" %>
<% end %>